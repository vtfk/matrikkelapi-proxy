   </xs:complexType>

   <xs:complexType name="KoordinatkvalitetKodeIdList">
      <xs:sequence>
         <xs:element name="item" type="KoordinatkvalitetKodeId" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>


    <xs:complexType name="KoordinatsystemKode">
