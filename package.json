{
  "name": "kartverket-matrikkel-api",
  "version": "1.0.0",
  "description": "Eksponerer ofte brukte funksjoner i Kartverkets SOAP Matrikkelen-API via REST",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "standardx --fix && cross-env NODE_ENV=development nodemon app.js",
    "debug": "cross-env NODE_ENV=development nodemon test.js",
    "standard": "standardx --fix",
    "test": "standardx --fix && jest",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karleinarb/kartverket-matrikkel-api.git"
  },
  "keywords": [
    "Kartverket",
    "Matrikkel",
    "Matrikkelen"
  ],
  "author": "Karl-Einar Bergstr√∏m",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karleinarb/kartverket-matrikkel-api/issues"
  },
  "homepage": "https://github.com/karleinarb/kartverket-matrikkel-api#readme",
  "devDependencies": {
    "audit": "0.0.6",
    "cross-env": "7.0.3",
    "jest": "27.2.1",
    "morgan": "1.10.0",
    "nodemon": "2.0.12",
    "snyk": "1.833.0",
    "standardx": "7.0.0"
  },
  "dependencies": {
    "@vtfk/utilities": "^1.0.0",
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "passport": "^0.4.1",
    "passport-headerapikey": "^1.2.2",
    "prettify-xml": "^1.2.0",
    "sjablong": "^1.0.33",
    "swagger-ui-express": "^4.3.0",
    "xml-query": "^1.5.0",
    "xml-reader": "^2.4.3",
    "xml2js": "^0.4.23",
    "xmldoc": "^1.1.2",
    "yamljs": "^0.3.0"
  },
  "standardx": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testMatch": [
      "**/test/**/*test.[jt]s?(x)",
      "**/?(*.)+.(spec|test).[jt]s?(x)"
    ]
  }
}
